<h1>{{info.title}}</h1>
<p class="odxl-info">
    Version: <span class="odxl-info-version">{{info.version}}</span>{{#if externalDocs}}, {{#with externalDocs}}{{>opendxl/externalDocs}}{{/with}}{{/if}}
</p>
<p>{{md info.description}}</p>

{{#if solutions}}
    {{>opendxl/solutions solutions=solutions}}
{{/if}}
{{#if services}}
    {{>opendxl/services services=services}}
{{/if}}
{{#if events}}
    {{>opendxl/events events=events}}
{{/if}}
{{#if requests}}
    {{>opendxl/requests requests=requests}}
{{/if}}
{{#if definitions}}
    {{>opendxl/definitions definitions=definitions}}
{{/if}}

